<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="apiaryanalysis.dialogs.DialogAddApiaryController">
   <bottom>
      <ToolBar BorderPane.alignment="CENTER">
        <items>
            <Button mnemonicParsing="false" text="Annuler" />
          <Button mnemonicParsing="false" text="Enregistrer" />
        </items>
      </ToolBar>
   </bottom>
   <center>
      <GridPane hgap="10.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" />
          <ColumnConstraints hgrow="SOMETIMES" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Nom" />
            <Label text="Numéro d'ordre" GridPane.rowIndex="1" />
            <Label text="Numéro de dossier" GridPane.rowIndex="2" />
            <TextField fx:id="textFieldName" GridPane.columnIndex="1" />
            <TextField fx:id="testFieldOrderNumber" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="testFieldFileNumber" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </GridPane>
   </center>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</BorderPane>
